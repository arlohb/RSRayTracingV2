{
  "name": "rsraytracingv2",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start-linux": "cargo run --release --target x86_64-unknown-linux-gnu",
    "start-web": "node server.js",
    "build:wasm": "wasm-pack build --target web --out-dir dist/pkg",
    "build": "npm run build:wasm && webpack build ./index.js --mode development -o dist --output-filename index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlohb/RSRayTracingV2.git"
  },
  "dependencies": {
    "comlink": "^4.3.1",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "wasm-pack": "^0.10.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  }
}
